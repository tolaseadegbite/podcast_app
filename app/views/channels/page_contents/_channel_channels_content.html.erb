<%= render partial: 'channels/channel_header' %>
            
<div class="container my-3">
    <% if @owned_channels.present? %>
        <h5>Other channels by <%= @channel.name %></h5>
        <div class="row mt-3">
            <%= render @owned_channels %>
        </div>
    <% else %>
        <p>There are no more channels</p>
    <% end %>
</div>